{
  "app-id": "io.github.Symphytum",
  "branch": "stable",
  "runtime": "org.kde.Platform",
  "sdk": "org.kde.Sdk",
  "runtime-version": "5.11",
  "command": "symphytum",
  "finish-args": [
    "--socket=wayland",
